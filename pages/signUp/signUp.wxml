<view>
  <view class="signUp">
    <button class="signUp-btn" bindtap="sendResume">
      <image src="../resource/images/submit_info.png" style="height:50rpx; width:50rpx;">
      </image>
      <view>确认报名</view>
    </button>
  </view>

  <view class="contest-info">
    <view class="title-font">{{contest_name}}</view>
  </view> 

  <view class="signUp-info">
    <view class="signUp-item">姓名：<input type="text" value="{{username}}"></input></view>
    <view class="signUp-item">学号：<input type="text" value="{{stu_id}}"></input></view>
    <view class="signUp-item">学校：<input type="text" value="{{school}}"></input></view>
    <view class="signUp-item">院系：<input type="text" value="{{department}}"></input></view>
  </view>
</view>
