@import "../common/lib/weui.wxss";

.resume-info {
  background-color: #E1F9FD;
  display: flex;
  flex-direction: row;
  padding: 15rpx;
}
.send-resume {
  background-color: #E1F9FD;
  display: flex;
  flex-direction: row;
  justify-content: center;
  font-size: 24rpx;
  margin: 0;
  padding-top: 10rpx;
  border: #EEE solid 2rpx;
}
.resume-info {
  display: flex;
  flex-direction: column;
  background-color: #E1F9FD;
}
.resume-item {
  display: flex;
  flex-direction: row;
  background-color: #fff;
  color: #888888;
  font-size: 30rpx;
  margin-top: 30rpx;
  padding: 15rpx 25rpx;
  border: 2rpx solid #ccc;
}